<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Your Memo's</title>
    <!-- <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/css/materialize.css"> -->
    <link rel="stylesheet" href="stylesheets/canvas.css">
  </head>
  <body>
    <div class="" id="">
      <h2 align='center'>Write Your Memo's</h2>
      <hr>
      <form class="" action="/users/add" method="post">
        <table class="striped" style="width:80%" align='center'>
          <thead>
            <tr>

              <td><b>ID</b></td>
              <td><b>NAME</b></td>
              <td><b>TITLE</b></td>
              <td><b>STATUS</b></td>
              <td><b>EMAIL</b></td>
              <td><b>CREATED AT</b></td>
              <td><b>ACTION</b></td>

            </tr>
          </thead>
          <tbody>

            <%
              alltodo.forEach((value)=>{
            %>
            <tr>

              <td><%= value.id%></td>
              <td><%= value.name%></td>
              <td><%= value.title%></td>
              <td><%= value.is_complete%></td>
              <td><%= value.email  %></td>
              <td><%= value.createdAt  %></td>

              <td><a href="/edit/<%=value.id%>">EDIT</a> | <a href="/delete/<%=value.id%>">DELETE</a></td>

            </tr>
            <%
            })
            %>


          </tbody>
        </table>

      </form>
    </div>
  </body>
</html>